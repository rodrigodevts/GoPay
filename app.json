{
  "expo": {
    "name": "gopay",
    "slug": "gopay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1A202C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rodrigodevjs.gopay"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1A202C"
      },
      "package": "com.rodrigodevjs.gopay"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
